{
  "name": "nfty chat - Discord Theme",
  "description": "A Discord theme for nfty chat.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://nftychat.xyz/*"], 
      "css" : ["custom.css"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/img/logo.png"
  }
}